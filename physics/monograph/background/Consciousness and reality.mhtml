</span><span><span class="hljs-keyword">\[</span></span><span>
</span><span><span class="hljs-keyword">\max</span></span><span> </span><span><span class="hljs-keyword">\Re</span></span><span><span class="hljs-keyword">\,</span></span><span><span class="hljs-keyword">\sigma</span></span><span><span class="hljs-keyword">\big</span></span><span>(</span><span><span class="hljs-keyword">\mathcal</span></span><span>{A}(k)</span><span><span class="hljs-keyword">\big</span></span><span>)
</span><span><span class="hljs-keyword">\;</span></span><span><span class="hljs-keyword">\le</span></span><span><span class="hljs-keyword">\;</span></span><span> -</span><span><span class="hljs-keyword">\,</span></span><span><span class="hljs-keyword">\mu</span></span><span> </span><span><span class="hljs-keyword">\;</span></span><span>-</span><span><span class="hljs-keyword">\;</span></span><span> </span><span><span class="hljs-keyword">\kappa</span></span><span><span class="hljs-built_in">_</span></span><span><span class="hljs-keyword">\Psi</span></span><span><span class="hljs-keyword">\,</span></span><span><span class="hljs-keyword">\rho</span></span><span><span class="hljs-keyword">\,</span></span><span>|k|</span><span><span class="hljs-built_in">^</span></span><span>2.
