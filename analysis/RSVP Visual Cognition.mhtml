        shear = torch.norm(self.curl(self.v), dim=</span><span><span class="hljs-number">1</span></span><span>, keepdim=</span><span><span class="hljs-literal">True</span></span><span>)
        Σ = F.relu(ρ * div_v.unsqueeze(</span><span><span class="hljs-number">1</span></span><span>) - </span><span><span class="hljs-number">0.5</span></span><span> * shear)
        </span><span><span class="hljs-keyword">return</span></span><span> Σ

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">step</span></span><span>(</span><span><span class="hljs-params">self</span></span><span>):
        adv_S = self.divergence(self.S * self.v)
        diff_S = self.laplacian(self.S)
        Σ = self.entropy_source()

        self.S = self.S + self.dt * (-adv_S + diff_S + Σ)

        grad_Φ = torch.gradient(self.Φ.squeeze(</span><span><span class="hljs-number">1</span></span><span>), spacing=(self.dx, self.dx, self.dx), dim=(</span><span><span class="hljs-number">1</span></span><span>,</span><span><span class="hljs-number">2</span></span><span>,</span><span><span class="hljs-number">3</span></span><span>))
        grad_Φ = torch.stack(grad_Φ, dim=</span><span><span class="hljs-number">1</span></span><span>)

        curl_v = self.curl(self.v)
        helicity_term = self.beta * self.Φ * torch.</span><span><span class="hljs-built_in">sum</span></span><span>(self.v * curl_v, dim=</span><span><span class="hljs-number">1</span></span><span>, keepdim=</span><span><span class="hljs-literal">True</span></span><span>)

        self.Φ = self.Φ + self.dt * self.laplacian(self.Φ) + self.dt * helicity_term
        self.v = self.v + self.dt * (-grad_Φ + self.curl(self.v))

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">run</span></span><span>(</span><span><span class="hljs-params">self, steps=<span class="hljs-number">100</span></span></span><span>):
        </span><span><span class="hljs-keyword">for</span></span><span> _ </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(steps):
            self.step()
