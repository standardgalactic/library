    </span><span><span class="hljs-keyword">for</span></span><span> radius </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(</span><span><span class="hljs-number">5</span></span><span>, </span><span><span class="hljs-number">30</span></span><span>, </span><span><span class="hljs-number">5</span></span><span>):  </span><span><span class="hljs-comment"># Every 5 pixels, increasing</span></span><span>
        color_intensity = </span><span><span class="hljs-built_in">int</span></span><span>(</span><span><span class="hljs-number">255</span></span><span> - (radius * </span><span><span class="hljs-number">5</span></span><span>))  </span><span><span class="hljs-comment"># Dim as it expands</span></span><span>
        cv2.circle(frame, (pos, </span><span><span class="hljs-number">50</span></span><span>), radius, (color_intensity, color_intensity, color_intensity), </span><span><span class="hljs-number">1</span></span><span>)

    frames_stretched.append(frame)

</span><span><span class="hljs-comment"># Display frames to visualize the effect</span></span><span>
