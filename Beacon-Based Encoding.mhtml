</span><span><span class="hljs-keyword">for</span></span><span> i, ax </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">enumerate</span></span><span>(axes.flatten()):
    </span><span><span class="hljs-keyword">if</span></span><span> i &lt; </span><span><span class="hljs-built_in">len</span></span><span>(frames_stretched):
        ax.imshow(frames_stretched[i])
        ax.set_title(</span><span><span class="hljs-string">f"Frame <span class="hljs-subst">{i+<span class="hljs-number">1</span></span></span></span><span>}")
    ax.axis(</span><span><span class="hljs-string">"off"</span></span><span>)

plt.suptitle(</span><span><span class="hljs-string">"Pixel Stretching &amp; Red/Blue Shift Encoding Motion &amp; Temperature"</span></span><span>)
