scene = np.ones(scene_size, dtype=np.uint8) * </span><span><span class="hljs-number">30</span></span><span>  </span><span><span class="hljs-comment"># Dark background</span></span><span>

</span><span><span class="hljs-comment"># Define objects with different properties (position, type, velocity, temperature, planned motion)</span></span><span>
objects = [
    {</span><span><span class="hljs-string">"pos"</span></span><span>: (</span><span><span class="hljs-number">40</span></span><span>, </span><span><span class="hljs-number">75</span></span><span>), </span><span><span class="hljs-string">"type"</span></span><span>: </span><span><span class="hljs-string">"solid"</span></span><span>, </span><span><span class="hljs-string">"velocity"</span></span><span>: </span><span><span class="hljs-number">2</span></span><span>, </span><span><span class="hljs-string">"temp"</span></span><span>: </span><span><span class="hljs-number">200</span></span><span>, </span><span><span class="hljs-string">"planned"</span></span><span>: </span><span><span class="hljs-literal">True</span></span><span>},
    {</span><span><span class="hljs-string">"pos"</span></span><span>: (</span><span><span class="hljs-number">75</span></span><span>, </span><span><span class="hljs-number">75</span></span><span>), </span><span><span class="hljs-string">"type"</span></span><span>: </span><span><span class="hljs-string">"hollow"</span></span><span>, </span><span><span class="hljs-string">"velocity"</span></span><span>: </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-string">"temp"</span></span><span>: </span><span><span class="hljs-number">100</span></span><span>, </span><span><span class="hljs-string">"planned"</span></span><span>: </span><span><span class="hljs-literal">False</span></span><span>},
    {</span><span><span class="hljs-string">"pos"</span></span><span>: (</span><span><span class="hljs-number">110</span></span><span>, </span><span><span class="hljs-number">75</span></span><span>), </span><span><span class="hljs-string">"type"</span></span><span>: </span><span><span class="hljs-string">"organic"</span></span><span>, </span><span><span class="hljs-string">"velocity"</span></span><span>: </span><span><span class="hljs-number">3</span></span><span>, </span><span><span class="hljs-string">"temp"</span></span><span>: </span><span><span class="hljs-number">255</span></span><span>, </span><span><span class="hljs-string">"planned"</span></span><span>: </span><span><span class="hljs-literal">True</span></span><span>},
]

</span><span><span class="hljs-comment"># Create beacon visualization with concentric signals encoding object properties</span></span><span>
</span><span><span class="hljs-keyword">for</span></span><span> obj </span><span><span class="hljs-keyword">in</span></span><span> objects:
    x, y = obj[</span><span><span class="hljs-string">"pos"</span></span><span>]
    velocity_factor = obj[</span><span><span class="hljs-string">"velocity"</span></span><span>] * </span><span><span class="hljs-number">5</span></span><span>  </span><span><span class="hljs-comment"># Exaggerate for effect</span></span><span>
    color_shift = obj[</span><span><span class="hljs-string">"temp"</span></span><span>]  </span><span><span class="hljs-comment"># Encode temperature via red intensity</span></span><span>

    </span><span><span class="hljs-comment"># Draw object base color</span></span><span>
    cv2.circle(scene, (x, y), </span><span><span class="hljs-number">5</span></span><span>, (color_shift, </span><span><span class="hljs-number">50</span></span><span>, </span><span><span class="hljs-number">255</span></span><span> - color_shift), -</span><span><span class="hljs-number">1</span></span><span>)  </span><span><span class="hljs-comment"># Red/Blue shift encoding</span></span><span>

    </span><span><span class="hljs-comment"># Draw beacon layers based on object properties</span></span><span>
    </span><span><span class="hljs-keyword">for</span></span><span> radius </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(velocity_factor, velocity_factor + </span><span><span class="hljs-number">30</span></span><span>, </span><span><span class="hljs-number">10</span></span><span>):
        intensity = </span><span><span class="hljs-built_in">int</span></span><span>(</span><span><span class="hljs-number">255</span></span><span> - (radius * </span><span><span class="hljs-number">4</span></span><span>))  </span><span><span class="hljs-comment"># Dims outward</span></span><span>
        </span><span><span class="hljs-keyword">if</span></span><span> obj[</span><span><span class="hljs-string">"type"</span></span><span>] == </span><span><span class="hljs-string">"hollow"</span></span><span>:
            intensity = </span><span><span class="hljs-built_in">max</span></span><span>(</span><span><span class="hljs-number">100</span></span><span>, intensity - </span><span><span class="hljs-number">50</span></span><span>)  </span><span><span class="hljs-comment"># Make hollow objects more distinct</span></span><span>

        </span><span><span class="hljs-keyword">if</span></span><span> obj[</span><span><span class="hljs-string">"planned"</span></span><span>]:
            cv2.circle(scene, (x, y), radius, (intensity, intensity, </span><span><span class="hljs-number">255</span></span><span>), </span><span><span class="hljs-number">1</span></span><span>)  </span><span><span class="hljs-comment"># Planned motion emits blue</span></span><span>

        </span><span><span class="hljs-keyword">else</span></span><span>:
            cv2.circle(scene, (x, y), radius, (intensity, </span><span><span class="hljs-number">255</span></span><span>, intensity), </span><span><span class="hljs-number">1</span></span><span>)  </span><span><span class="hljs-comment"># Unplanned emits green</span></span><span>

</span><span><span class="hljs-comment"># Display result</span></span><span>
