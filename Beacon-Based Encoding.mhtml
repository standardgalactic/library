</span><span><span class="hljs-comment"># Generate frames with pixel stretching &amp; color encoding</span></span><span>
frames_stretched = []
</span><span><span class="hljs-keyword">for</span></span><span> i, pos </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">enumerate</span></span><span>(object_positions):
    frame = background_template.copy()

    </span><span><span class="hljs-comment"># Stretch pixels based on speed (motion blur effect)</span></span><span>
    stretch_amount = </span><span><span class="hljs-built_in">int</span></span><span>(object_speed[i] * </span><span><span class="hljs-number">2</span></span><span>)  </span><span><span class="hljs-comment"># Exaggerate for effect</span></span><span>
    </span><span><span class="hljs-keyword">for</span></span><span> j </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(-stretch_amount, stretch_amount + </span><span><span class="hljs-number">1</span></span><span>):
        x = np.clip(pos + j, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">99</span></span><span>)
